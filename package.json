{
  "name": "@ozerohax/assistagents",
  "version": "0.2.0",
  "description": "npx installer for OpenCode agents/skills and key files",
  "license": "Apache-2.0",
  "type": "module",
  "bin": {
    "assistagents": "dist/cli.js",
    "assistagents-setup": "dist/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "templates"
  ],
  "engines": {
    "node": ">=20.12.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/cli.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "tsx --test",
    "prepack": "npm run build"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.2.1",
    "archiver": "^7.0.1"
  },
  "devDependencies": {
    "@types/archiver": "^7.0.0",
    "@types/node": "^20.19.33",
    "tsup": "^8.0.2",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4"
  }
}
